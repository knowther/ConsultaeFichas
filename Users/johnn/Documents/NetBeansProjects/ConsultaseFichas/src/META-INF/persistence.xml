<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ConsultaseFichasPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>model.Horariosdisponiveis</class>
    <class>model.Pacientesconsulta</class>
    <class>model.Medicos</class>
    <class>view.Consulta</class>
    <class>model.Estado</class>
    <class>model.Cidade</class>
    <class>model.Tipodeficha</class>
    <class>view.Caixa</class>
    <class>model.Movimentacao</class>
    <class>model.Categoria</class>
    <class>model.Movimentacaoconsultorio</class>
    <class>model.Movimentacaodraedna</class>
    <class>model.Agendamentopermcath</class>
    <class>model.User</class>
    <class>model.Consulta</class>
    <class>model.Regulacao</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/sultest?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
    </properties>
  </persistence-unit>
</persistence>
